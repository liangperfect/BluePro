<?xml version="1.0" encoding="UTF-8"?>

    <shape xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- 设置背景填充色 -->


    <solid android:color="#8470FF"/>

    <!-- 设置边框宽度为1dp，边框颜色 -->


    <stroke android:color="#000" android:width="1dp"/>

    <!-- 设置按钮圆角半径为25dp -->


    <corners android:bottomLeftRadius="0dp" android:topLeftRadius="0dp" android:topRightRadius="0dp" android:bottomRightRadius="0dp"/>

</shape>